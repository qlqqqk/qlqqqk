    "#구름 뱀이지나간자리\n",
    "height,width = map(int,input().split())\n",
    "\n",
    "for i in range(1,height+1):\n",
    "\tif i % 2 ==1: print(\"#\"*width)\n",
    "\telse:\n",
    "\t\tif (i // 2) %2==1: print(\".\"*(width-1)+\"#\")\n",
    "\t\telse: print(\"#\"+\".\"*(width-1))"
   ]
